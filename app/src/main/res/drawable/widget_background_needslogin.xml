<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/background_norm"/>
            <corners android:radius="@dimen/widget_background_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="@color/protonWidgetNeedsLoginGradientStart"
                android:centerColor="@color/protonWidgetNeedsLoginGradientMiddle"
                android:endColor="@color/protonWidgetNeedsLoginGradientEnd"
                android:centerY="0.5"
                android:angle="270"/>
            <corners android:radius="@dimen/widget_background_radius" />
        </shape>
    </item>
</layer-list>
