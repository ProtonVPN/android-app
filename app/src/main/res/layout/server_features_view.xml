<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iconP2P"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/ic_proton_arrows_switch"
        android:layout_marginEnd="4dp"/>

    <ImageView
        android:id="@+id/iconTor"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/ic_proton_brand_tor"
        android:layout_marginEnd="4dp"/>

    <ImageView
        android:id="@+id/iconStreaming"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/ic_proton_play"
        android:layout_marginEnd="4dp"/>

    <ImageView
        android:id="@+id/iconSmartRouting"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:srcCompat="@drawable/ic_proton_globe"
        android:layout_marginEnd="4dp"/>
</merge>
